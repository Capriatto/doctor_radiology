<?xml version="1.0"?>
<openerp>
	<data noupdate="1">

		<!-- Sequence type for attention -->
		<record model="ir.sequence.type" id="seq_type_attention_radio">
			<field name="name">Radiology Sequence</field>
			<field name="code">radiology.sequence</field>
		</record>

		<!--Sequence for attention -->
		<record model="ir.sequence" id="seq_attention_radio">
			<field name="name">Radiology Sequence</field>
			<field name="code">radiology.sequence</field>
			<field name="padding" eval="4"/>
			<field name="prefix">RAD-%(month)s%(y)s-</field>
		</record>

	</data>
</openerp>
